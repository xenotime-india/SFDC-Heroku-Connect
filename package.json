{
  "name": "salesforce-heroku-connect",
  "version": "1.0.0",
  "description": "This is a assignment for traction.",
  "main": "index.js",
  "author": "Xenotime <sandeep@appirio.com>",
  "private": true,
  "engines": {
    "node": "8.9.4"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "dependencies": {
    "bcrypt": "^2.0.1",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "config": "^1.30.0",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.16.3",
    "express-limiter": "^1.6.1",
    "glob": "^7.1.2",
    "helmet": "^3.12.1",
    "js-yaml": "^3.11.0",
    "jsonwebtoken": "^7.4.2",
    "knex": "^0.14.6",
    "lodash": "^4.17.10",
    "mock-knex": "^0.4.0",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.17",
    "ms": "^2.0.0",
    "node-fetch": "^2.1.2",
    "node-sass": "^4.9.0",
    "passport": "^0.4.0",
    "passport-oauth2-refresh": "^1.0.0",
    "passport-salesforce": "^0.0.2",
    "passport-saml": "^0.33.0",
    "pg": "^7.4.3",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-inlinesvg": "^0.8.1",
    "react-lightning-design-system": "^2.5.7",
    "react-moment-countdown": "^0.3.0",
    "react-moment-proptypes": "^1.5.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "react-scripts": "^1.0.13",
    "react-table": "^6.8.6",
    "redis": "^2.8.0",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sequelize": "^4.37.10",
    "swagger-express-mw": "^0.7.0",
    "uuid": "^3.2.1",
    "winston": "^2.4.2"
  },
  "lint-staged": {
    "*.js": [
      "./node_modules/.bin/prettier-eslint --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "build": "react-scripts build",
    "heroku-postbuild": "./node_modules/.bin/react-scripts build",
    "start": "node index.js",
    "precommit": "lint-staged",
    "develop:api": "./node_modules/.bin/nodemon --exec \"yarn install && node\" --inspect=0.0.0.0:5858 --nolazy index.js",
    "develop:client": "REACT_APP_REDUX_DEBUGGING_ENABLED=true ./node_modules/.bin/react-scripts start",
    "swagger:edit": "swagger_swagger_fileName=api/swagger/swagger.yaml swagger project edit -p 3002 --host 0.0.0.0 -s",
    "test:client": "jest --config jest.client.json --coverage",
    "test:client:watch": "jest --config jest.client.json --watchAll --coverage",
    "test:api": "jest --config jest.server.json --coverage",
    "test:api:watch": "jest --config jest.server.json --watchAll --coverage",
    "test": "npm run test:client && npm run test:api",
    "prettify:api": "find ./api/**/*.js | xargs ./node_modules/.bin/prettier --write --single-quote",
    "prettify:client": "find ./src/**/*.js | xargs ./node_modules/.bin/prettier --write --single-quote"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react-app": "^3.1.1",
    "enzyme": "^3.3.0",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.8.2",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.3",
    "mock-knex": "^0.4.0",
    "node-mocks-http": "^1.7.0",
    "nodemon": "^1.17.5",
    "object-assign": "^4.1.1",
    "prettier": "^1.13.4",
    "prettier-eslint": "^8.8.1",
    "prettier-eslint-cli": "^4.7.1",
    "prettier-stylelint": "^0.4.2",
    "promise": "^8.0.1",
    "raf": "^3.4.0",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.4.0",
    "regenerator-runtime": "^0.11.1",
    "stylelint-config-standard": "^18.2.0",
    "whatwg-fetch": "^2.0.4"
  }
}
